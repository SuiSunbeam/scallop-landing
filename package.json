{
  "name": "scallop-landing",
  "version": "2.0.2",
  "description": "Scallop home page",
  "author": "team@scallop.io",
  "homepage": "https://www.scallop.io",
  "bugs": "https://github.com/scallop-io/scallop-landing/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scallop-io/scallop-landing.git"
  },
  "license": "Apache Licence 2.0",
  "private": true,
  "engines": {
    "node": ">=18.15.0",
    "pnpm": ">=8.4.0"
  },
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "commit": "commit",
    "prepare": "husky install",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --check '**/*.{html,ts,tsx,json,css,md,cjs}'",
    "format:fix": "prettier --write '**/*.{html,ts,tsx,json,css,md,cjs}'",
    "style": "stylelint **/*.{html,css}",
    "style:fix": "stylelint **/*.{html,css} --fix"
  },
  "dependencies": {
    "@fontsource/poppins": "^5.0.2",
    "@headlessui/react": "^1.7.15",
    "@heroicons/react": "^2.0.18",
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-countup": "^6.4.2",
    "react-dom": "^18.2.0",
    "react-ga4": "^2.1.0",
    "react-plx": "^2.1.2",
    "react-router-dom": "^6.12.0",
    "web-vitals": "^3.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@commitlint/prompt-cli": "^17.6.5",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/nesting": "0.0.0-insiders.565cd3e",
    "@tailwindcss/typography": "^0.5.9",
    "@types/react": "^18.2.9",
    "@types/react-dom": "^18.2.4",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@vitejs/plugin-legacy": "^4.0.4",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "cssnano": "^6.0.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "happy-dom": "^9.20.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.24",
    "postcss-html": "^1.5.0",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^11.2.2",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "stylelint": "^15.7.0",
    "stylelint-config-standard": "^33.0.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vite-plugin-chunk-split": "^0.4.7",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-node-polyfills": "^0.8.2",
    "vite-plugin-pwa": "^0.16.3",
    "vitest": "^0.32.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npx run format:fix",
      "npx run lint:fix"
    ],
    "**/*.css": [
      "npx run format:fix",
      "npx run style:fix"
    ],
    "package.json": [
      "npx run format:fix"
    ],
    "*.md": [
      "npx run format:fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowAny": [
        "@babel/*"
      ],
      "ignoreMissing": [
        "csstype",
        "terser"
      ],
      "allowDeprecated": {
        "sourcemap-codec": "*",
        "rollup-plugin-terser": "*",
        "stable": "*"
      }
    }
  }
}
